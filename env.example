# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Backend API URL
# For local: http://localhost:5000
# For Vercel: https://your-backend-app.vercel.app
VITE_API_BASE_URL=http://localhost:5000

# ZEGOCLOUD Video Calling Credentials
# Get these from: https://console.zegocloud.com/
VITE_ZEGO_APP_ID=your_zego_app_id_here
VITE_ZEGO_SERVER_SECRET=your_zego_server_secret_here

